{
  "name": "reading-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "nx exec -- next build",
    "pack": "nx exec -- next-on-pages",
    "start": "next start",
    "lint": "nx exec -- next lint && tsc",
    "test:e2e": "nx exec -- playwright test",
    "db:push": "drizzle-kit push:mysql --config ./drizzle.config.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@planetscale/database": "^1.13.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^7.88.0",
    "@swc/helpers": "~0.5.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "jotai": "^2.6.0",
    "jotai-location": "^0.5.2",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.5",
    "next": "^14.0.4",
    "node-html-parser": "^6.1.11",
    "otp-io": "^1.2.6",
    "qrcode": "^1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.2",
    "@nx-aws-plugin/nx-aws-cache": "^3.2.0",
    "@nx/eslint-plugin": "^17.2.6",
    "@nx/js": "17.2.6",
    "@nx/next": "^17.2.6",
    "@nx/react": "17.2.6",
    "@playwright/test": "^1.40.1",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.10.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "0.20.7",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "nx": "17.2.6",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "pack": {
        "outputs": [
          "{projectRoot}/.vercel"
        ]
      },
      "test:e2e": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
