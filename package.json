{
  "name": "reading-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "nx exec -- next build",
    "pack": "nx exec -- next-on-pages",
    "start": "next start",
    "lint": "nx exec -- next lint && tsc",
    "test:e2e": "nx exec -- playwright test",
    "db:push": "drizzle-kit push:mysql --config ./drizzle.config.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@planetscale/database": "^1.11.0",
    "@sentry/nextjs": "^7.83.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "jotai": "^2.6.0",
    "jotai-location": "^0.5.2",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.5",
    "next": "^14.0.3",
    "node-html-parser": "^6.1.11",
    "otp-io": "^1.2.6",
    "qrcode": "^1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.0",
    "@nx-aws-plugin/nx-aws-cache": "^3.1.0",
    "@playwright/test": "^1.40.0",
    "@types/node": "20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "0.20.6",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "nx": "17.1.3",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "pack": {
        "outputs": [
          "{projectRoot}/.vercel"
        ]
      },
      "test:e2e": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
